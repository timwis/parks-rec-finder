{
  "extends": [
    "plugin:vue/recommended",
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "vue/html-closing-bracket-newline": "error",
    "vue/html-closing-bracket-spacing": ["error", {
      "selfClosingTag": "never"
    }]
  },
  "globals": {
    "describe": true,
    "before": true,
    "beforeEach": true,
    "it": true,
    "cy": true
  }
}
